{
    "name": "humweb/laravel-widgets",
    "description": "Laravel Widgets package",
    "keywords": [
        "humweb",
        "widgets",
        "laravel"
    ],
    "homepage": "https://github.com/humweb/laravel-widgets",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Shofner",
            "email": "ryun@humboldtweb.com",
            "homepage": "http://humboldtweb.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "humweb/widgets": "1.0"
    },
    "autoload": {
        "psr-4": {
            "Humweb\\LaravelWidgets\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Humweb\\Widgets\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
